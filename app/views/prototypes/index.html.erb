<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
    <h2>こんにちは、<%= link_to current_user.name, user_path(current_user) %> さん</h2>
    <% else %>
    <h2></h2>
    <% end %>

    <div class="card__wrapper">
     <%= render partial: "prototypes/prototype", collection: @prototypes, as: :prototype %>
        
    </div>
  </div>
</main>